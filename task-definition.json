{
  "family": "project-06",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],

  "cpu": "1024",
  "memory": "2048",

  "executionRoleArn": "arn:aws:iam::905702888382:role/ecsTaskExecutionRole",

  "containerDefinitions": [
    {
      "name": "train-ticket-reservation",
      "image": "<IMAGE>",
      "essential": true,

      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/trainbook",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
